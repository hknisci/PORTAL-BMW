{
  "enabled": true,
  "provider": "splunk",
  "intervalSeconds": 30,
  "products": [
    "httpd",
    "nginx",
    "jboss",
    "websphere",
    "ctg",
    "hazelcast",
    "provenir"
  ],
  "dynatrace": {
    "baseUrl": "",
    "apiToken": ""
  },
  "splunk": {
    "baseUrl": "https://SPLUNK_HOST:8089",
    "token": "SPLUNK_TOKEN",
    "authScheme": "Bearer",
    "indexes": [
      "main"
    ],
    "windowMinutes": 15
  }
}